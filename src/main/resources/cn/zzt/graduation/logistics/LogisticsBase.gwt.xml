<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='Base'>
	<inherits name='com.google.gwt.user.User' />
	<inherits name="cn.xiaobu.web.citybeauty.beauty" />
	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<inherits name='cn.zzt.graduation.logistics.client.theme.Flex'></inherits>

	<set-property name="gwt.logging.enabled" value="FALSE" />

	<set-property name="mgwt.formfactor" value="phone" />
	<set-property name="user.agent" value="safari" />

	<set-property name="city.projectname" value="jhx" />

	<!--phonegap配置.打包APP版本时需要切换到TRUE,其他情况改为FALSE -->
	<set-property name="phonegap.enabled" value="TRUE" />

	<source path='client' />
	<source path='shared' />
</module>
